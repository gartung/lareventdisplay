<!--                                                                                  -->
<!--  Include art::Wrapper lines for objects that we would like to put into the event -->
<!--  Include the non-wrapper lines for all objects on the art::Wrapper lines and     -->
<!--  for all objects that are data members of those objects.                         -->


<lcgdict>
 <!-- support classes (e.g., elements of data product classes) -->
   <!-- for art association metadata -->
 <class name="std::vector<unsigned short>"                                />
 
 <!-- data product classes -->
 <class name="TEveElement" ClassVersion="0" persistent="false"/>
 
 <!-- art pointers and pointer vectors -->
 <class name="art::Ptr<TEveElement>"                            />
 <class name="art::PtrVector<TEveElement>"                      />
 <class name="std::vector< art::PtrVector<TEveElement> >"       />
 
 <!-- pairs and tuples for associations -->
 <class name="std::pair< art::Ptr<TEveElement>, art::Ptr<recob::Cluster> >"    />
 <class name="std::pair< art::Ptr<TEveElement>, art::Ptr<recob::Hit>        >" />
 <class name="std::pair< art::Ptr<TEveElement>, art::Ptr<recob::PFParticle> >" />
 <class name="std::pair< art::Ptr<TEveElement>, art::Ptr<recob::Shower>     >" />
 <class name="std::pair< art::Ptr<TEveElement>, art::Ptr<recob::SpacePoint> >" />
 <class name="std::pair< art::Ptr<TEveElement>, art::Ptr<recob::Track>      >" />
 <class name="std::pair< art::Ptr<TEveElement>, art::Ptr<recob::Vertex>     >" />
 <class name="std::pair< art::Ptr<TEveElement>, art::Ptr<recob::EndPoint2D> >" />
 <class name="std::pair< art::Ptr<TEveElement>, art::Ptr<recob::Event> >"      />
 <class name="std::pair< art::Ptr<TEveElement>, art::Ptr<recob::Seed>       >" />
 <class name="std::pair< art::Ptr<TEveElement>, art::Ptr<recob::PCAxis>     >" />
 
 <!-- associations -->
 <class name="art::Assns<TEveElement,       recob::Cluster,    void>"          />
 <class name="art::Assns<TEveElement,       recob::Hit,        void>"          />
 <class name="art::Assns<TEveElement,       recob::PFParticle, void>"          />
 <class name="art::Assns<TEveElement,       recob::Shower,     void>"          />
 <class name="art::Assns<TEveElement,       recob::SpacePoint, void>"          />
 <class name="art::Assns<TEveElement,       recob::Track,      void>"          />
 <class name="art::Assns<TEveElement,       recob::Vertex,     void>"          />
 <class name="art::Assns<TEveElement,       recob::EndPoint2D, void>"          />
 <class name="art::Assns<TEveElement,       recob::Event,      void>"          />
 <class name="art::Assns<TEveElement,       recob::Seed,       void>"          />
 <class name="art::Assns<TEveElement,       recob::PCAxis,     void>"          /> 

 <!-- vectors for data products -->
 <class name="std::vector<TEveElement>"       />
 
 <!-- art data product wrappers -->
 <class name="art::Wrapper< std::vector< TEveElement                        > >" />
 <class name="art::Wrapper< std::vector< art::PtrVector< TEveElement        > >" />
 
 <!-- art association wrappers -->
 <class name="art::Wrapper< art::Assns<TEveElement,       recob::Cluster,    void> >"          />
 <class name="art::Wrapper< art::Assns<TEveElement,       recob::Hit,        void> >"          />
 <class name="art::Wrapper< art::Assns<TEveElement,       recob::PFParticle, void> >"          />
 <class name="art::Wrapper< art::Assns<TEveElement,       recob::Shower,     void> >"          />
 <class name="art::Wrapper< art::Assns<TEveElement,       recob::SpacePoint, void> >"          />
 <class name="art::Wrapper< art::Assns<TEveElement,       recob::Track,      void> >"          />
 <class name="art::Wrapper< art::Assns<TEveElement,       recob::Vertex,     void> >"          />
 <class name="art::Wrapper< art::Assns<TEveElement,       recob::EndPoint2D, void> >"          />
 <class name="art::Wrapper< art::Assns<TEveElement,       recob::Event,      void> >"          />
 <class name="art::Wrapper< art::Assns<TEveElement,       recob::Seed,       void> >"          />
 <class name="art::Wrapper< art::Assns<TEveElement,       recob::PCAxis,     void> >"          /> 

