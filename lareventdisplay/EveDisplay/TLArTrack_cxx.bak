//LArSoft includes
#include "lareventdisplay/EveDisplay/TLArTrack.h"
#include "lareventdisplay/EveDisplay/TLArElement.h"

//ROOT includes
#include "TEveTrack.h"

//c++ includes
#include <string>

namespace eved
{
  TLArTrack::TLArTrack(std::string label): TEveTrack(), TLArElement(label)
  {
  }

  TLArTrack& TLArTrack::operator=(const TLArTrack& track)
  {
    TEveTrack(&track);
    TLArElement::operator=(track);
    return *this;
  }
}
